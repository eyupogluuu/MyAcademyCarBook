@model Service
@{
    ViewData["Title"] = "addService";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h1>Yeni Hizmet Girişi</h1>
<form method="post">
    <label>İkon</label>
    <input type="text" asp-for="icon" class="form-control" style="border:groove" />
    <span asp-validation-for="icon" class="text-danger"></span>
    <br />
    <label>Tittle</label>
    <input type="text" asp-for="tittle" class="form-control" style="border:groove" />
    <span asp-validation-for="tittle" class="text-danger"></span>
    <br />
    <label>Açıklama</label>
    <input type="text" asp-for="descreption" class="form-control" style="border:groove" />
    <span asp-validation-for="descreption" class="text-danger"></span>
    <br />
    <button class="btn btn-primary">Kaydet</button>
</form>

